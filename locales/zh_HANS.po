msgid "Text to display in the bottom center"
msgstr "在底部中央显示的文字"

#. :src: bpy.types.BurnInProperties.bottom_left
msgid "Text to display in the bottom left corner"
msgstr "在左下角显示的文字"

#. :src: bpy.types.BurnInProperties.bottom_right
msgid "Text to display in the bottom right corner"
msgstr "在右下角显示的文字"

#. :src: bpy.types.BurnInProperties.color
